#include <cstdio>
#include <iostream>
#include <cstring>
#include <algorithm>
#include <cmath>
#include <vector>
using namespace std;
#define max(a,b) (a<b?b:a)
#define F(i,L,R) for (int i = L; i < R; i++)
#define ll long long
class AlienAndGame
{
    public:
        inline int calc(int a, int b){
            if (a < b) return a*a;
            else return b*b;
        }
        int getNumber(vector <string> board) {
            int n = board.size(), m = board[0].length();
            int h[n+1][2];
            F(i,0,n) h[i][0] = 1;
            h[n][0] = 0;
            int now = 0;
            int pre;
            F(j,1,m){
                F(i,0,n) h[i][j] = (board[i][j] == board[i][j-1] ? h[i][j-1]+1 : 1);
                h[n][j] = 0;
            }
            //F(i,0,n)
            //   F(j,0,m) cout << h[i][j] << (j == m-1? '\n':' ');
            int q[n+2], w[n+2];
            int top;
            ll ans = 0;
            F(j,0,m){ 
                top = 0;
                q[top] = -1;
                F(i,0,n+1){
                    if(h[i][j] > q[top]) 
                        q[++top] = h[i][j] , w[top] = 1; 
                    else{ 
                        ll cnt = 0; 
                        while(h[i][j] <= q[top]) { 
                            ans = max(ans ,calc(cnt+w[top],q[top])); 
                            cnt += w[top--]; 
                        } 
                        q[++top] = h[i][j]; 
                        w[top] = cnt+1; 
                    } 
                } 
            }
            return ans;
        }
};
